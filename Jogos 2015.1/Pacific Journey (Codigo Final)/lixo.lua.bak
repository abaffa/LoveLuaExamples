function lixo_spawn(vetor)
  x = love.math.random(60,540)
  tab.insert(vetor, {
      dirx = 1,
      px = x,
      hitbox_px = x,
      py = 800,
      random = love.math.random(1,3),
      rot = 0,
      sx = 0.5,
      sy = 0.5,
      ox = 0,
      oy = 0,
      tipo = 4,
      width = 90,
      height = 135
    })
end

function lixo_move(dt, tab)
  px_spawn = tab.px
  tab.px = tab.px + (100 * tab.dirx * dt)
  if tab.px > px_spawn + (70 * tab.random) or tab.px>600-54.5 then -- 54.5 Ã© a largura da imagem do tab dentro da escala
    tab.random=love.math.random(1,3)
    tab.dirx=-1
  elseif tab.px<px_spawn-(50*tab.random) or tab.px <0 then
    tab.random=love.math.random(1,3)
    tab.dirx=1
  end
  tab.py=tab.py-(50*dt)
end