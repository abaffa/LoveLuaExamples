function missel_spawn(vetor)
  table.insert(vetor, {
      px=sub.px+90,
      py=sub.py+40,
      rot=2.25,
      hitbox_px = px,
      hitbox_py = py,
      sx = 0.3,
      sy = 0.3,
      ox = 0,
      oy = 0,
      width= 30,
      height = 30,
      collided = false
    })
end

function missel_move(dt, table)
  table.py = table.py + 500 * dt
end
