function missil_spawn(vetor)
  tab.insert(vetor, {
      px= sub.px + 90,
      py = sub.py + 40,
      rot = 2.25,
      hitbox_px = px,
      hitbox_py = py,
      sx = 0.3,
      sy = 0.3,
      ox = 0,
      oy = 0,
      width = 30,
      height = 30
    })
end

function missil_move(dt, tab)
  tab.py = tab.py + 500 * dt
end
